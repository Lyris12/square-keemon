{
  "name": "square-keemon",
  "version": "1.0.19",
  "type": "module",
  "description": "Draws a square containing a customizable rainbow gradient",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lyris12/square-keemon.git"
  },
  "keywords": [
    "square",
    "react",
    "draw"
  ],
  "author": "Clara Grace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lyris12/square-keemon/issues"
  },
  "homepage": "https://github.com/Lyris12/square-keemon#readme",
  "files": [
    "src/lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/square-keemon.umd.js",
  "module": "./dist/square-keemon.es.js",
  "types": "./dist/square-keemon.d.ts",
  "exports": {
    ".": {
      "import": "./dist/square-keemon.es.js",
      "require": "./dist/square-keemon.umd.js",
      "types": "./dist/square-keemon.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --write",
    "format": "npm run prettier && npm run lint",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "3.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4"
  }
}
