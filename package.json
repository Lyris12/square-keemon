{
  "name": "square-keemon",
  "version": "1.0.0",
  "type": "module",
  "description": "Draws a square containing a customizable rainbow gradient",
  "main": "./dist/square-keemon.umd.js",
  "module": "./dist/square-keemon.es.js",
  "exports": {
    ".": {
      "import": "./dist/square-keemon.es.js",
      "require": "./dist/square-keemon.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lyris12/square-keemon.git"
  },
  "keywords": [
    "square",
    "react",
    "draw"
  ],
  "author": "Clara Grace",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Lyris12/square-keemon/issues"
  },
  "homepage": "https://github.com/Lyris12/square-keemon#readme",
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "vite": "^5.0.0"
  }
}
